{
  "manifest_version": 2,
  "name": "Informa Booking Monitor",
  "version": "0.1.0",
  "description": "Chrome extension to monitor Informa woocommerce bookings",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Informa Booking Monitor"
  },
  "permissions":  ["storage"],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk=' https://cdn.jsdelivr.net; object-src 'self'"
}
